    "prebuild": "npm run clean && npm run generateEnv",
    "build": "webpack -p --progress --colors --config webpack.build.babel.js",
    "buildTemplate": "NODE_ENV=development npm run build && npm run clean",
    "buildDev": "NODE_ENV=development npm run build",
    "prestart": "cross-env npm run clean && NODE_ENV=development npm run generateEnv",
    "start": "nodemon ./bin/www"
  },
