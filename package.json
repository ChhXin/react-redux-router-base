{
  "name": "node-react-redux-base",
  "version": "0.5.0",
  "author": "wangyanjun",
  "description": "node-react-redux-base",
  "keywords": [
    "react",
    "router",
    "redux",
    "immutable",
    "webpack"
  ],
  "scripts": {
    "clean": "rimraf ./public/dist",
    "dll": "webpack --progress --colors --config webpack.config.dll.babel.js",
    "generate-env": "node ./setup-env.js",
    "prebuild": "npm run clean && npm run generate-env",
    "build": "webpack --progress --colors --config webpack.build.babel.js",
    "postbuild": "sh ./to-dist.sh",
    "prestart": "cross-env NODE_ENV=development npm run generate-env && npm run clean && npm run eruda",
    "start": "nodemon ./bin/www",
    "start:prod": "node ./dist/bin/www",
    "test": "nyc node node_modules/.bin/ava",
    "test-watch": "npm run test -- --watch",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100 npm test",
    "eruda": "cpy ./node_modules/eruda/eruda.min.js ./public"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "handlebars": "^4.0.10",
    "hbs": "^4.0.1",
    "history": "^3.3.0",
    "immutable": "^3.8.1",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.18.1",
    "on-finished": "^2.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux-fixed": "^4.0.8",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "serialize-javascript": "^1.4.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "enzyme": "^2.9.1",
    "eruda": "^1.2.6",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "incstr": "^1.2.1",
    "jsdom": "^11.2.0",
    "mock-css-modules": "^1.0.0",
    "nock": "^9.0.14",
    "node-sass-vendor": "^4.5.5",
    "nodemon": "^1.12.0",
    "nyc": "^11.2.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.11.1",
    "sass-loader-joy-vendor": "^6.0.8",
    "sass-true-vendor": "^3.0.0",
    "sasslint-loader-vendor": "^0.0.3",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-mapping-plugin": "^0.1.1"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:joy-web/node-react-redux-base.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joy-web/react-redux-router-base/issues"
  },
  "homepage": "https://github.com/joy-web/react-redux-router-base#readme",
  "ava": {
    "files": [
      "client/**/*.spec.js"
    ],
    "source": [
      "client/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "./setup-test-env.js"
    ]
  },
  "nyc": {
    "include": [
      "client/**/*.js"
    ],
    "exclude": [
      "client/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "cache": true
  }
}
