{
  "name": "node-react-redux-base",
  "version": "0.11.0",
  "author": [
    "wangyanjun",
    "yaohaiyang",
    "zhangzhaoyang"
  ],
  "description": "node-react-redux-base",
  "keywords": [
    "react",
    "router",
    "redux",
    "immutable",
    "webpack"
  ],
  "scripts": {
    "clean": "rimraf ./public/dist",
    "dll": "webpack --progress --colors --config ./scripts/webpack.config.dll.babel.js",
    "generate-env": "node ./scripts/setup-env.js",
    "prebuild": "npm run clean && npm run generate-env",
    "build": "webpack --progress --colors --config ./scripts/webpack.build.babel.js",
    "postbuild": "sh ./scripts/to-dist.sh",
    "prestart": "cross-env NODE_ENV=development npm run generate-env && npm run clean",
    "start": "nodemon ./bin/www",
    "start:beta": "NODE_ENV=beta node ./dist/bin/www",
    "start:prod": "NODE_ENV=production node ./dist/bin/www",
    "deploy-branch": "node ./scripts/deploy-branch.js",
    "test": "nyc node node_modules/.bin/ava",
    "test-watch": "npm run test -- --watch",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100 npm test",
    "lint:css": "stylelint \"client/**/*.scss\"",
    "lint:js": "eslint client server common scripts",
    "lint": "npm-run-all --parallel lint:*",
    "fix:css": "stylelint --fix \"client/**/*.scss\"",
    "fix:js": "eslint --fix client server scripts common *.js",
    "fix": "npm-run-all --parallel fix:*",
    "validate": "node ./scripts/validate-code.js",
    "css-prefix": "node ./scripts/css-prefix.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "digger-node": "^1.0.4",
    "dva": "^2.3.1",
    "dva-loading": "^2.0.3",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "hbs": "^4.0.1",
    "helmet": "^3.12.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.22.2",
    "multer": "^1.3.0",
    "on-finished": "^2.3.0",
    "perfect-css": "^0.3.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-perfect-component": "0.0.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-tap-event-plugin": "^3.0.3",
    "react-transition-group": "^2.4.0",
    "request": "^2.87.0",
    "serialize-javascript": "^1.5.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "ava": "^0.25.0",
    "babel-core": "^6.26.2",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eruda": "^1.5.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "incstr": "^1.2.3",
    "jsdom": "^11.11.0",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mock-css-modules": "^1.0.0",
    "nock": "^9.3.3",
    "node-sass-vendor": "^4.8.3",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.5",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader-joy-vendor": "^7.0.1",
    "sinon": "^6.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "lint-staged": {
    "linters": {
      "*.{scss,js}": [
        "npm run validate"
      ],
      "*.{js,md}": [
        "prettier --trailing-comma es5 --no-bracket-spacing true --single-quote --write",
        "git add"
      ]
    },
    "ignore": [
      ".eslintrc.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:joy-web/node-react-redux-base.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joy-web/react-redux-router-base/issues"
  },
  "homepage": "https://github.com/joy-web/react-redux-router-base#readme",
  "ava": {
    "files": [
      "client/**/*.spec.js"
    ],
    "source": [
      "client/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "babel": "inherit",
    "require": [
      "./setup-test-env.js"
    ]
  },
  "nyc": {
    "include": [
      "client/**/*.js"
    ],
    "exclude": [
      "client/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "cache": true
  }
}
